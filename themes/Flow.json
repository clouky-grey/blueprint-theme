    "tokenColors": [
        {
            "settings": {
                "background": "#262724",
                "foreground": "#C7C7C7"
            }
        },
        {
            "name": "[Comment] - Block Comment: /* */",
            "scope": [
                "comment.block",
                "punctuation.definition.comment.end",
                "punctuation.definition.comment.begin"
            ],
            "settings": {
                "foreground": "#4E545A", // 4C545B 4E545A
                // "foreground": "#525252",
                "fontStyle": "bold"
                
            }
        },
        {
            "name": "[Comment] - Doc Comment: /** */",
            "scope": [
                "comment.block.documentation",
                "punctuation.definition.comment.begin.documentation",
                "punctuation.definition.comment.end.documentation"
            ],
            "settings": {
                "foreground": "#4E545A", // 4C545B 4E545A
                // "foreground": "#525252",
                "fontStyle": "bold"
            }
        },
        {
            "name": "[Comment] - Line Comment: //",
            "scope": [
                "comment.line"
            ],
            "settings": {
                //"foreground": "#525252",
                "foreground": "#4E545A", // 4E5459 4C545B 4E545A
                //"fontStyle": "italic"
                "fontStyle": "bold"
            }
        },
